<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youlai.boot.scene.mapper.TriggerMapper">

    <!-- 获取触发器分页列表 -->
    <select id="getTriggerPage" resultType="com.youlai.boot.scene.model.vo.TriggerVO">
        SELECT
                id,
                scene_id,
                type,
                device_ids,
                category_id,
                cron,
                threshold,
                threshold_logic
        FROM
            trigger
        <where>
        </where>
    </select>

</mapper>
