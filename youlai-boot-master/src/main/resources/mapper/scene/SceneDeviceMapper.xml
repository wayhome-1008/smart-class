<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youlai.boot.scene.mapper.SceneDeviceMapper">

    <!-- 获取场景设备分页列表 -->
    <select id="getSceneDevicePage" resultType="com.youlai.boot.scene.model.vo.SceneDeviceVO">
        SELECT
                id,
                category_id,
                category_name,
                script_id,
                scene_id,
                type,
                source
